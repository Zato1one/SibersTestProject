@model IEnumerable<SibersTestProject.Common.Model.UserModel>
@{
    ViewBag.Title = "UserList";
}
<h2>UserList</h2>
<table class="table">
    <tr>
        <th>Логин</th>
        <th>Имя</th>
        <th>Фамилия</th>
        <th>Email</th>
        <th>Роли</th>
    </tr>
    @foreach (var user in Model)
            {
        <tr>
            <td>@user.UserName</td>
            <td>@user.FirstName</td>
            <td>@user.LastName</td>
            <td>@user.Email</td>
            <td>
                @foreach (var role in user.Role)
            {
                @role <br>
            }
        </td>
    </tr>
    }
</table>
