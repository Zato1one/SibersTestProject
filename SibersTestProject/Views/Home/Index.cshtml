@model IEnumerable<SibersTestProject.Common.Model.UserModel>
@{
    ViewBag.Title = "UserList";
}
<h2>UserList</h2>
@Html.ActionLink("UploadPhoto", "Upload", "Photo")
@Html.ActionLink("PhotoList", "Index", "Photo")
@Html.ActionLink("GalleryCreate", "Create", "Gallery")
@Html.ActionLink("GalleryList", "Index", "Gallery")
@Html.ActionLink("AllPublicGalleryList", "AllPublicGallery", "Gallery")

<table class="table">
    <tr>
        <th>Логин</th>
        <th>Имя</th>
        <th>Фамилия</th>
        <th>Email</th>
        <th>Роли</th>
        <th></th>
    </tr>
    @foreach (var user in Model)
            {
        <tr>
            <td>@user.UserName</td>
            <td>@user.FirstName</td>
            <td>@user.LastName</td>
            <td>@user.Email</td>
            <td>
                @foreach (var role in user.Roles)
            {
                @role <br>
            }
        </td>
            <td>
                @Html.RouteLink("ChangeRole", new { controller = "Admin", action = "ChangeRole", id = user.EntityId })
            </td>
    </tr>
    }
</table>
